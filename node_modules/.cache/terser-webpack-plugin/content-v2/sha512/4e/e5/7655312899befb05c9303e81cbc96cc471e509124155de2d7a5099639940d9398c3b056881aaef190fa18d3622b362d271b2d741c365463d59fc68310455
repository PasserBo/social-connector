etFAQData }) => {\r\n  const handleFaqFilter = e => {\r\n    setSearchTerm(e.target.value)\r\n    getFAQData(e.target.value)\r\n  }\r\n\r\n  return (\r\n    <div id='faq-search-filter'>\r\n      <Card\r\n        className='faq-search'\r\n        style={{\r\n          backgroundImage: `url(${require('@src/assets/images/banner/banner.png').default})`\r\n        }}\r\n      >\r\n        <CardBody className='text-center'>\r\n          <h2 className='text-primary'>Let's answer some questions</h2>\r\n          <CardText className='mb-2'>or choose a category to quickly find the help you need</CardText>\r\n          <Form className='faq-search-input' onSubmit={e => e.preventDefault()}>\r\n            <InputGroup className='input-group-merge'>\r\n              <InputGroupText>\r\n                <Search size={14} />\r\n              </InputGroupText>\r\n              <Input value={searchTerm} onChange={e => handleFaqFilter(e)} placeholder='search faq...' />\r\n            </InputGroup>\r\n          </Form>\r\n        </CardBody>\r\n      </Card>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default FaqFilter\r\n","// ** Icons Imports\r\nimport { PhoneCall, Mail } from 'react-feather'\r\n\r\n// ** Reactstrap Imports\r\nimport { Row, Col, Card, CardBody } from 'reactstrap'\r\n\r\nconst FaqContact = () => {\r\n  return (\r\n    <div className='faq-contact'>\r\n      <Row className='mt-5 pt-75'>\r\n        <Col className='text-center' sm='12'>\r\n          <h2>You still have a question?</h2>\r\n          <p className='mb-3'>\r\n            If you cannot find a question in our FAQ, you can always contact us. We will answer to you shortly!\r\n          </p>\r\n        </Col>\r\n        <Col sm='6'>\r\n          <Card className='text-center faq-contact-card shadow-none py-1'>\r\n            <CardBody>\r\n              <div className='avatar avatar-tag bg-light-primary mb-2 mx-auto'>\r\n                <PhoneCall size={18} />\r\n              </div>\r\n              <h4>+ (810) 2548 2568<